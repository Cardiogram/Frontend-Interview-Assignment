{
  "name": "cardiogram-product-interview",
  "description": "Cardiogram product onsite interview assignment",
  "version": "0.4.0",
  "main": "src/index.js",
  "author": "Cardiogram <hello@cardiogr.am>",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "git://github.com/Cardiogram/onsite-interview-project.git"
  },
  "bugs": {
    "url": "https://github.com/Cardiogram/onsite-interview-project/issues"
  },
  "scripts": {
    "server": "node server",
    "dev": "react-scripts start",
    "dev:server": "nodemon server",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb",
      "plugin:prettier/recommended"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js"
          ]
        }
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:5000",
  "dependencies": {
    "ajv": "6.7.0",
    "cors": "2.8.5",
    "d3": "5.8.1",
    "express": "4.16.4",
    "isomorphic-fetch": "2.2.1",
    "normalize.css": "8.0.1",
    "prop-types": "15.7.0",
    "react": "16.8.0",
    "react-dom": "16.8.0",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.4"
  },
  "devDependencies": {
    "nodemon": "1.19.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.6.0",
    "eslint-plugin-prettier": "3.0.1",
    "prettier": "1.16.1"
  }
}
